  const int bitmapSize = sizeof(epd_bitmap_Bitmap);                // 圖片數據的總大小
  const int frameSize = 1024;                                      // 每幀大小
  const int frameCount = (bitmapSize + frameSize - 1) / frameSize; // 計算總幀數

  for (int i = 0; i < frameCount; i++) {
    int offset = i * frameSize;                                                          // 當前幀的偏移量
    int length = (offset + frameSize <= bitmapSize) ? frameSize : (bitmapSize - offset); // 計算當前幀大小

    // 清空畫面
    display.clearDisplay();

    // 繪製當前幀數據
    display.drawBitmap(0, 0, epd_bitmap_Bitmap + offset, SCREEN_WIDTH, SCREEN_HEIGHT, SSD1306_WHITE);

    // 顯示畫面
    display.display();

    // 延遲一段時間以模擬動畫（例如 100 毫秒）
    delay(50);
  }
}
